<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup Condition="'$(MonoMacResourcePrefix)' == '' And '$(UseXamMacFullFramework)' == ''">
    <None Include="$(MSBuildThisFileDirectory)x86\unix\libSDL2.so">
      <Link>x86\unix\libSDL2.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(MSBuildThisFileDirectory)x86\win32nt\SDL2.dll">
      <Link>x86\win32nt\SDL2.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
	<None Include="$(MSBuildThisFileDirectory)x64\unix\libSDL2.so">
      <Link>x64\unix\libSDL2.so</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(MSBuildThisFileDirectory)x64\win32nt\SDL2.dll">
      <Link>x64\win32nt\SDL2.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup Condition="'$(MonoMacResourcePrefix)' != '' Or '$(UseXamMacFullFramework)' != ''">
    <BundleResource Include="$(MSBuildThisFileDirectory)x64\osx\libSDL2.dylib">
      <Link>x64\osx\libSDL2.dylib</Link>
    </BundleResource>
  </ItemGroup>
</Project>
